<templateSet group="myTemplates">
  <template name="log" value="console.log($stuff$);" description="console.log()" toReformat="false" toShortenFQNames="true">
    <variable name="stuff" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="iife" value="(function () {&#10;    &quot;use strict&quot;;&#10;    &#10;    $code$&#10;})();" description="Immediately Invoked Function Expression" toReformat="false" toShortenFQNames="true">
    <variable name="code" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="psl" value="private static org.slf4j.Logger logger = org.slf4j.LoggerFactory.getLogger($currentClass$.class);" description="private static Logger" toReformat="false" toShortenFQNames="true">
    <variable name="currentClass" expression="className()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>